{
  "name": "cracking-the-coding-interview",
  "version": "1.0.0",
  "description": "Crackin the coding interview problems",
  "main": "index.js",
  "scripts": {
    "test": "vitest ./javascript/*",
    "test-coding": "vitest ./javascript/*",
    "strings": "vitest ./javascript/0* --reporter=default --reporter=./silverReporter.ts",
    "lists": "vitest ./javascript/1* --reporter=default --reporter=./silverReporter.ts",
    "stacks": "vitest ./javascript/2* --reporter=default --reporter=./silverReporter.ts",
    "trees": "vitest ./javascript/3* --reporter=default --reporter=./silverReporter.ts",
    "recursion": "vitest ./javascript/4* --reporter=default --reporter=./silverReporter.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsconfig/node20": "^20.1.4",
    "chalk": "^5.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "vitest": "^2.0.5"
  }
}
